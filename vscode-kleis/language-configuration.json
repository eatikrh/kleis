{
  "comments": {
    "lineComment": "//",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" },
    { "open": "/*", "close": "*/", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*\\{[^}]*$|^.*\\([^)]*$|^.*\\[[^\\]]*$|^\\s*(structure|implements|data|define|match)\\b.*$",
    "decreaseIndentPattern": "^\\s*[}\\])]|^\\s*\\}"
  },
  "onEnterRules": [
    {
      "beforeText": "^.*\\{[^}]*$",
      "afterText": "^\\s*\\}",
      "action": { "indent": "indentOutdent" }
    },
    {
      "beforeText": "^.*\\{[^}]*$",
      "action": { "indent": "indent" }
    },
    {
      "beforeText": "^\\s*(structure|implements|data)\\b.*\\{\\s*$",
      "action": { "indent": "indent" }
    }
  ],
  "folding": {
    "markers": {
      "start": "^\\s*//\\s*#?region\\b",
      "end": "^\\s*//\\s*#?endregion\\b"
    }
  },
